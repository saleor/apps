{
  "name": "saleor-app-cms",
  "version": "2.11.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit",
    "deploy": "tsx --require dotenv/config ./scripts/deploy.ts",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:debug": "APP_LOG_LEVEL=debug pnpm dev",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "graphql-codegen",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "next start",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@datocms/cma-client-browser": "2.0.0",
    "@hookform/resolvers": "^3.3.1",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/api-logs": "catalog:",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:",
    "@opentelemetry/instrumentation": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-logs": "catalog:",
    "@opentelemetry/sdk-trace-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "@saleor/app-sdk": "link:../../node_modules/@saleor/app-sdk",
    "@saleor/apps-logger": "workspace:*",
    "@saleor/apps-otel": "workspace:*",
    "@saleor/apps-shared": "workspace:*",
    "@saleor/apps-ui": "workspace:*",
    "@saleor/macaw-ui": "1.1.10",
    "@saleor/react-hook-form-macaw": "workspace:*",
    "@saleor/sentry-utils": "workspace:*",
    "@sentry/cli": "catalog:",
    "@sentry/nextjs": "catalog:",
    "@tanstack/react-query": "4.29.19",
    "@trpc/client": "10.43.1",
    "@trpc/next": "10.43.1",
    "@trpc/react-query": "10.43.1",
    "@trpc/server": "10.43.1",
    "@urql/exchange-auth": "2.1.4",
    "@vercel/otel": "catalog:",
    "@vitejs/plugin-react": "4.3.1",
    "contentful-management": "10.46.4",
    "dotenv": "16.3.1",
    "graphql": "16.7.1",
    "graphql-tag": "2.12.6",
    "jsdom": "^20.0.3",
    "next": "14.2.3",
    "p-ratelimit": "1.0.1",
    "qs": "6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-hook-form": "^7.43.9",
    "strapi-sdk-js": "2.3.4",
    "urql": "4.0.4",
    "usehooks-ts": "^2.9.1",
    "vite": "5.3.3",
    "vitest": "1.6.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typed-document-node": "5.0.5",
    "@graphql-codegen/typescript": "4.0.5",
    "@graphql-codegen/typescript-operations": "4.1.3",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/qs": "^6.9.7",
    "@types/react": "18.2.5",
    "@types/react-dom": "18.2.5",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "catalog:",
    "eslint-config-saleor": "workspace:*",
    "node-mocks-http": "^1.12.2",
    "tsx": "4.7.1",
    "typescript": "5.5.4"
  },
  "saleor": {
    "schemaVersion": "3.10"
  }
