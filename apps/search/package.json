    "@trpc/server": "10.43.1",
    "@types/debug": "^4.1.7",
    "@urql/exchange-auth": "^2.1.4",
    "algoliasearch": "4.19.1",
    "clsx": "^1.2.1",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "graphql": "16.7.1",
    "graphql-tag": "^2.12.6",
    "next": "13.4.8",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.43.9",
