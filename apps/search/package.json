    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "pnpm tsx ./scripts/migrations/run-webhooks-migration.ts",
    "start": "next start",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
