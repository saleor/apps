    "@vitejs/plugin-react": "4.0.4",
    "eslint": "8.46.0",
    "eslint-config-saleor": "workspace:*",
    "node-mocks-http": "^1.12.2",
    "tsx": "4.1.2",
    "typescript": "5.1.6",
    "vite": "4.4.8",
