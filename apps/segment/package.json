{
  "name": "saleor-app-segment",
  "version": "2.1.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc",
    "deploy": "tsx --require dotenv/config ./scripts/deploy.ts",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "graphql-codegen",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "pnpm tsx --require dotenv/config ./scripts/run-webhooks-migration.ts",
    "migrate:dry-run": "pnpm tsx --require dotenv/config ./scripts/run-webhooks-migration.ts --dry-run",
    "start": "next start",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.651.1",
    "@aws-sdk/lib-dynamodb": "3.651.1",
    "@aws-sdk/util-dynamodb": "3.651.1",
    "@hookform/resolvers": "^3.3.1",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/api-logs": "catalog:",
    "@opentelemetry/instrumentation": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-logs": "catalog:",
    "@opentelemetry/sdk-trace-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "@saleor/app-sdk": "link:../../node_modules/@saleor/app-sdk",
    "@saleor/apps-logger": "workspace:*",
    "@saleor/apps-otel": "workspace:*",
    "@saleor/apps-shared": "workspace:*",
    "@saleor/apps-ui": "workspace:*",
    "@saleor/macaw-ui": "catalog:",
    "@saleor/react-hook-form-macaw": "workspace:*",
    "@saleor/sentry-utils": "workspace:*",
    "@saleor/webhook-utils": "workspace:*",
    "@segment/analytics-node": "2.2.0",
    "@sentry/cli": "catalog:",
    "@sentry/nextjs": "catalog:",
    "@sentry/node": "catalog:",
    "@t3-oss/env-nextjs": "0.11.1",
    "@trpc/client": "catalog:",
    "@trpc/next": "catalog:",
    "@trpc/server": "catalog:",
    "@vercel/otel": "catalog:",
    "@vitejs/plugin-react": "4.3.1",
    "decimal.js-light": "2.5.1",
    "dotenv": "16.3.1",
    "dynamodb-toolbox": "1.8.2",
    "graphql": "16.7.1",
    "graphql-tag": "2.12.6",
    "modern-errors": "7.0.1",
    "modern-errors-serialize": "6.1.0",
    "neverthrow": "6.2.1",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "4.0.10",
    "react-hook-form": "^7.43.9",
    "urql": "catalog:",
    "usehooks-ts": "^2.9.1",
    "uuid": "11.0.4",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typed-document-node": "5.0.5",
    "@graphql-codegen/typescript": "4.0.5",
    "@graphql-codegen/typescript-operations": "4.1.3",
    "@graphql-codegen/typescript-urql": "4.0.0",
    "@graphql-typed-document-node/core": "3.2.0",
