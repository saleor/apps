{
  "name": "@saleor/webhook-utils",
  "version": "0.2.4",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "check-types": "tsc",
    "fetch-schema": "curl https://raw.githubusercontent.com/saleor/saleor/${npm_package_saleor_schemaVersion}/saleor/graphql/schema.graphql > graphql/schema.graphql",
    "generate": "graphql-codegen",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@saleor/app-sdk": "link:../../node_modules/@saleor/app-sdk",
    "@urql/exchange-auth": "2.1.4",
    "eslint": "catalog:",
    "graphql": "16.7.1",
    "graphql-tag": "2.12.6",
    "modern-errors": "7.0.1",
    "modern-errors-serialize": "6.1.0",
    "semver": "7.5.1",
    "urql": "4.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/schema-ast": "4.0.2",
    "@graphql-codegen/typed-document-node": "5.0.5",
    "@graphql-codegen/typescript": "4.0.5",
    "@graphql-codegen/typescript-operations": "4.1.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@saleor/eslint-config-apps": "workspace:*",
    "@saleor/typescript-config-apps": "workspace:*",
    "@types/semver": "7.5.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "catalog:",
    "graphql-config": "5.0.3",
    "typescript": "5.5.4",
    "urql": "4.0.4",
    "vite": "5.3.3",
    "vitest": "1.6.0"
  },
  "peerDependencies": {
    "next": "14.2.3"
  },
  "saleor": {
    "schemaVersion": "3.19"
  }
