{
  "name": "@saleor/sentry-utils",
  "version": "0.1.0",
  "scripts": {
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "devDependencies": {
    "@sentry/cli": "../../node_modules/@sentry/cli",
    "@sentry/nextjs": "../../node_modules/@sentry/nextjs",
    "eslint-config-saleor": "workspace:*",
    "typescript": "5.1.6"
  },
  "exports": {
