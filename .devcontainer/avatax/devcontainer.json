{
  "name": "AvaTax app",
  "dockerComposeFile": "docker-compose.yml",
  "service": "avatax",
  "workspaceFolder": "/app/apps/avatax",
  "forwardPorts": [3000, "dynamodb:8000"],
  "portsAttributes": {
    "3000": {
      "label": "AvaTax app"
    },
    "dynamodb:8000": {
      "label": "Local DynamoDB"
    }
  },
  "postCreateCommand": "./scripts/setup-dynamodb.sh",
  "features": {
    "ghcr.io/devcontainers/features/aws-cli": {}
  }
}
